{
  "version": 2,
  "crons": [
    {
      "path": "/api/notifications/check-all",
      "schedule": "0 0 * * *"
    }
  ],
  "routes": [
    {
      "src": "/api(/.*)?",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist-server/**"
    }
  }
}